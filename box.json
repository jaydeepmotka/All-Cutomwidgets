      }
    ],
    "properties": {
      "color": {
        "type": "string",
        "description": "Background color",
        "default": "red"
      },
      "name": {
        "type": "string",
        "description": "Name",
        "default": ""
      },
      "opacity": {
        "type": "number",
        "description": "Opacity",
        "default": 1
      },
      "radius": {
        "type": "string",
        "description": "Radius",
        "default": "yellow"
      },
      "width": {
        "type": "integer",
        "default": 100
      },
      "height": {
        "type": "integer",
        "default": 100
      }
    },
    "methods": {
      "setColor": {
        "description": "Sets the background color.",
        "parameters": [
          {
            "name": "newColor",
            "type": "string",
            "description": "The new background color"
          }
        ],
        "body": "this.color = newColor;"
      },
      "setRadis": {
        "description": "Sets the border color.",
        "parameters": [
          {
            "name": "newRadius",
            "type": "string",
            "description": "The new border color"
          }
        ],
        "body": "this.radius = newRadius;"
      },
      "setOpacity": {
        "description": "Sets the box opacity.",
        "parameters": [
          {
            "name": "newOpacity",
            "type": "number",
            "description": "The new box opacity"
          }
        ],
        "body": "this.opacity = newOpacity;"
      },
      "setName": {
        "description": "Sets name",
        "parameters": [
          {
            "name": "newColor",
            "type": "string",
            "description": "The new background color"
          }
        ],
        "body": "this.name = newColor;"
      },
      "getColor": {
        "returnType": "string",
        "description": "Returns the background color.",
        "body": "return this.color;"
      },
      "getRadis": {
        "returnType": "string",
        "description": "Returns the border color.",
        "body": "return this.radius;"
      },
      "getOpacity": {
        "returnType": "number",
        "description": "Returns the box opacity.",
        "body": "return this.opacity;"
      },
      "getName": {
        "returnType": "string",
        "description": "Returns the background color.",
        "body": "return this.name;"
      },
      "getInfo": {
        "returnType": "ApplicationInfo",
        "description": "Returns the background color.",
        "body": "return Application.getInfo();"
      }
    },
    "events": {
      "onClick": { "description": "Called when the user clicks the Colored Box." }
    }
